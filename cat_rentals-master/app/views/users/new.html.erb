<h1>Sign Up</h1>

<form action="<%=users_url%>" method="post">
  <%= auth_token %>
  <label for="user_user_name"> input username</label>
  <input type="text" name="user[user_name]" id="user_user_name"><br>
  <label for="user_password"> input password</label>
  <input type="password" name="user[password]" id="user_password">
  <br>
  <input type="submit" value="Submit">
</form>
