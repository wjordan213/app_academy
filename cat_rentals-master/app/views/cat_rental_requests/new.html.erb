<ul>
  <% flash[:errors].to_a.each do |error| %>
    <li><%= error %></li>
  <% end %>
</ul>
<form action="<%=cat_rental_requests_url%>" method="post">
  <%= auth_token %>

<label for="cat_id">Select a Cat!</label><br>
 <select name="cat_rental_request[cat_id]">
   <option value = ''>our cats!</option>
   <% @cats.each do |cat| %>
    <option value= "<%=cat.id%>" > <%= cat.name %></option>
    <% end %>
 </select><br>
 <label for="cat_rental_request_start_date">Start Date</label><br>
 <input name="cat_rental_request[start_date]" id="cat_rental_request_start_date" type="date", value= <%=@cat_rental_request.start_date %>>
 </input><br>

 <label for="cat_rental_request_end_date">End Date</label><br>
 <input name="cat_rental_request[end_date]" id="cat_rental_request_end_date" type="date", value= <%=@cat_rental_request.end_date %>>
 </input>

 <button >Submit</button>
</form>
